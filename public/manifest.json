{
  "name": "링크드인 경력계산기 - React",
  "description": "링크드인 경력계산기 - React",
  "version": "0.1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.medium.com/*", "*://*.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
   "activeTab"
  ]
}